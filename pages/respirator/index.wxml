<!--pages/respirator/index.wxml-->
<view class="respiratorWrap">
  <swiper class="merchant_img" indicatorDots="{{indicatorDots}}" indicator-color="#fafafa" indicator-active-color="#FDE50D" circular="{{circular}}" interval="{{interval}}" autoplay="{{autoplay}}" duration="{{duration}}">
    <block wx:for="{{bannerList}}" wx:key="id">
      <swiper-item>
        <image src="{{item.banner}}" class="slide-image" />
      </swiper-item>
    </block>
  </swiper>
  <view class="imgWrap">
    <image class="img" src="/resource/img/kz/banner1-1.jpg"></image>
  </view>
  <view class="moneyWrap">
    <view class="money">
      <view class="money_left">￥188.00</view>
      <!-- <view class="money_right">￥188.00</view> -->
    </view>
    <view class="text_description">腾飞科技防病毒病菌民用时尚舒适男女口罩采用N95标准滤材过滤率98.3%以上1包10只</view>
    <view class="imgWrap" style="width:96%;">
      <image class="img" src="/resource/img/kz/expressage.jpg"></image>
    </view>
  </view>
  <view class="kzWrap">
    <image class="img1" src="/resource/img/kz/xc1.png"></image>
    <image class="img2" src="/resource/img/kz/xc2.jpg"></image>
    <image class="img3" src="/resource/img/kz/xc3.jpg"></image>
  </view>
  <view class="btnWrap">
    <navigator url="orderform/index" class="btn">
      <icon class="iconfont icon-dianji icon-xiaofeimingxi kz"></icon>
      <text style="margin-left:6rpx;">订单</text>
    </navigator>
    <navigator url="payments/index" class="btn">
      <icon class="iconfont icon-dianji kz"></icon>
      <text style="margin-left:6rpx;">立即购买</text>
    </navigator>
  </view>
</view>
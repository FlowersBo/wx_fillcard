/* pages/generalize/index.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #EEE9E9;
}
.generalizeWrap{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.generalize{
  width: 96%;
}
.cardList{
  width: 100%;
  height: 270rpx;
  background-color: #fff;
  border-radius: 15rpx;
  margin-top: 20rpx;
  box-sizing: border-box;
  padding: 15rpx;
}
.cardList:last-child{
  margin-bottom: 20rpx;
}
.cardWrap{
  width: 100%;
  display: flex;
}
.card_img{
  width: 280rpx;
  height: 180rpx;
  margin-right: 15rpx;
}
.img{
  width: 100%;
  height: 100%;
  border-radius: 15rpx;
}
.card_details{
  flex: 1;
  min-width: 0;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.card_name{
  font-size: 32rpx;
  color: #C27D53;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
  margin-bottom: 40rpx;
}
.piceWrap{
  display: flex;
  justify-content: space-between;
}
.pice{
  font-size: 28rpx;
  color: #8A8A8A;
}
.money{
  color: #C27D53;
}
.address{
  width: 100%;
  margin-top: 10rpx;
  font-weight: bold;
  font-size: 36rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
}
.boxWrap{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.box{
  height: 500rpx;
  margin-top: 40%;
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #757575;
}
.icon-icon_wuxiaoxi-{
  font-size: 400rpx;
}
/* pages/user/orderForm/orderDetails/qrCode/index.wxss */
page {
  width: 100%;
  background-color: #f5f5f5;
}
.qrCodeWrap{
  width: 100%;
  height: 600rpx;
  background-color: #fff;
}
.qrCode{
  box-sizing: border-box;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.img{
  width: 400rpx;
  height: 400rpx;
}
.qrCode-img{
  width: 100%;
  height: 100%;
}
.qrCode-text{
  font-size: 32rpx;
  margin-top: 30rpx;
}
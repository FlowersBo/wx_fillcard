/* pages/user/bindMobile/index.wxss */
page{
  width: 100%;
  background-color: #f2f2f2;
}
.bindMobileWrap{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.bindMobile{
  width: 95%;
}
.bindMobile_header{
  margin: 30rpx 0 15rpx 0;
  font-size: 32rpx;
}
.list{
  width: 100%;
}
.list_child{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color: #ccc;
  background-color: #fff;
  border-bottom: 2rpx solid #f2f2f2;
  box-sizing: border-box;
  padding-left: 20rpx;
}
.btnWrap{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.btn{
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #FBE931;
  border-radius: 15rpx;
  font-size: 32rpx;
  margin-top: 50rpx;
}
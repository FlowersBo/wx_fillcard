/* pages/user/index.wxss */
page{
  width: 100%;
  background-color: #F5F5F5;
}
.container {
  min-height: 100%;
  position: relative;
} 
.header{
  width:100%;
  height: 300rpx;
  display:flex;/*设为伸缩容器*/  
  flex-flow:row;/*伸缩项目单行排列*/ 
}
.balance{
  height: 45rpx;
  line-height: 45rpx;
  background-color: #FDE60C;
  box-sizing: border-box;
  padding: 0 10rpx;
  border-radius: 6rpx;
  font-size: 36rpx;
  font-weight: bold;
  color: #002;
  margin-top: 90rpx;
}
.container .header .pic{
  width: 200rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.pic-img{
  width:150rpx;
  height:150rpx;
  border-radius: 50%;
  margin-top: 30rpx;
}

.container .header .pic .pic-name{
  width:100%;
  /* text-indent: 30rpx; */
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
  font-size: 28rpx;
  margin-top: 15rpx;
}

 /*
  九宫格容器布局样式
 */
.grid{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: -50rpx;
}


.grid-item-container {
  width: 94%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  border-radius: 25rpx;
  background-color: #FFF;
}
.grid-item-container:last-child{
  border-radius: 0 0 25rpx 25rpx;
}

.else-header{
  width: 94%;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 25rpx 25rpx 0 0;
  background-color: #fff;
  box-sizing: border-box;
  padding-left: 20rpx;
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 15rpx;
}
/*
  item容器样式
*/
.grid-item-child {
  display:flex;
  display: -webkit-flex;
  justify-content:center;
  flex-direction: column;
  flex-wrap: wrap;
  margin: 20rpx 0;
}
.imgWrap{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}
.img{
  width: 70rpx;
  height: 60rpx;
}


/*
 文本样式
*/
.grid-item-label {
  display:flex;
  display: -webkit-flex;
  justify-content:center;
  color: #666;
  font-size: 32rpx;
  margin-top: 20rpx;
}

/* 订单 */
.order{
  width: 94%;
  background-color: #fff;
  border-radius: 25rpx;
  margin-top: 15rpx;
}

.order-header{
  height: 60rpx;
  line-height: 60rpx;
  box-sizing: border-box;
  padding-left: 20rpx;
  font-size: 32rpx;
  font-weight: bold;
}

.order-content{
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 20rpx 90rpx;
}
.bargainirg{
  display: flex;
  align-items: center;
  justify-content: center;
}
.order-imgWrap{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.order-imgWrap image{
  width: 70rpx;
  height: 70rpx;
}
.order-text{
  color: #666;
  font-size: 32rpx;
  margin-top: 20rpx;
}




















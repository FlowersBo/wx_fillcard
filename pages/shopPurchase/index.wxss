.swiperWrap{
  position: relative;
}
.photoAlbum{
  position: absolute;
  right: 20rpx;
  bottom: 15rpx;
  height: 50rpx;
  box-sizing: border-box;
  padding: 0 15rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 25rpx;
  background-color: #200;
  opacity: 0.7;
  z-index: 100;
}
.icon-tupian{
  font-size: 36rpx;
  color: #fff;
  margin-right: 10rpx;
  margin-bottom: 15rpx;
}

.photoAlbumText{
  font-size: 28rpx;
  color: #fff;
}

.swiper-item {
  width: 100%;
  height: 500rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
}
/* 商家信息 */
.merchantMessage{
  width: 100%;
  background-color: #fff;
  box-sizing: border-box;
  padding: 10rpx;
  height: 250rpx;
  display: flex;
  align-items: center;
  margin: 20rpx 0;
}
.shopMessage {
  flex: 1;
  min-width: 0;
  padding-left: 20rpx; 
}
.shopMessage_child {
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  align-items: center;
}
.shopImg{
  width: 140rpx;
  height: 140rpx;
}
.shopImg image{
  width: 100%;
  height: 100%;
}
.shopMessageText{
  font-size: 28rpx;
  color: #200;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  word-break: break-all;
  margin-top: 11rpx;
}
.icon {
  margin-right: 10rpx;
  font-size: 30rpx;
  color: #c0c0c0;
}

.icon-iconfontxiangxia1copy19 {
  margin-left: auto;
}



.sellCard{
  width: 100%;
}

.cardListWrap{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.cardList{
  width: 100%;
}
.cardTitle{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  font-size: 36rpx;
  font-weight: bold;
  border-bottom: 1rpx solid #ddd;
  background-color: #fff;
  justify-content: center;
}
.icon-xuzhi1{
  font-size: 44rpx;
  margin-right: 10rpx;
  margin-bottom: 6rpx;
}

.cards{
  width: 100%;
  height: 280rpx;
  background-color: #fff;
  margin-bottom: 10rpx;
}
.cards:last-child{
  margin-bottom: 30rpx;
}

.cardChild{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  box-sizing: border-box;
  padding: 0 10rpx;
}
